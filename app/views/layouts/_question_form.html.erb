<%= form_for Question.new do |f| %> 
  <%= f.label :title %>
  <%= f.text_field :title, :placeholder => "Title" %>
  <%= f.label :body %>
  <%= f.text_area :body, :placeholder => "Enter your question here" %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.submit "Submit" %>
<% end %>

